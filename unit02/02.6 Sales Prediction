fun main(args: Array<String>) {
    val totalSales = 4.6
    val eastCoastPercentage = 0.62

    val eastCoastSales = totalSales * eastCoastPercentage

    println("The East Coast division is predicted to generate $$eastCoastSales of sales this year.")
}
